package unreal;

import "unreal/gdk/core_types.schema";

type UnrealPayload {
    uint32 offset = 1;
    uint32 rpc_index = 2;
    bytes rpc_payload = 3;
}

component UnrealClientRPCEndpoint {
    id = 100008;
    event UnrealPayload unreliable_client_to_server_rpc;
    command UnrealRPCCommandResponse reliable_server_to_client_rpc(UnrealPayload);
}

component UnrealServerRPCEndPoint {
    id = 100009;
    event UnrealPayload unreliable_server_to_client_rpc;
    command UnrealRPCCommandResponse reliable_client_to_server_rpc(UnrealPayload);
}

component UnrealMulticastRPCEndpoint {
    id = 100010;
    event UnrealPayload unreliable_multicast_rpc;
}
